source=()
i=0
cnt=${#_pkglist[@]}
while [ $i -lt $cnt ]; do
	source=("${source[@]}"
		http://arm.konnichi.com/${_pkglist[$[i+2]]}/extra/os/i686/${_pkglist[$i]}-${_pkglist[$[i+1]]}.pkg.tar.${_pkglist[$[i+3]]}
	)
	i=$[i+4]
done
source=("${source[@]}")
